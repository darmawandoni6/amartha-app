{
  "name": "next-customer-web",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "local": "env-cmd -f ./.env.local next dev",
    "dev": "env-cmd -f ./.env.dev next dev",
    "uat": "env-cmd -f ./.env.uat next dev",
    "prod": "env-cmd -f ./.env.prod next dev",
    "build-exec": "cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/ && cp -r newrelic.js .next/standalone/",
    "build-local": "env-cmd -f ./.env.local next build && yarn build-exec",
    "build-dev": "env-cmd -f ./.env.dev next build && yarn build-exec",
    "build-uat": "env-cmd -f ./.env.uat next build && yarn build-exec",
    "build-prod": "env-cmd -f ./.env.production next build && yarn build-exec",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "lint-quiet": "next lint --quiet",
    "pre-commit": "lint-staged",
    "type-check": "tsc --noEmit",
    "test": "jest --changedSince=origin/main --coverage",
    "test:watch": "jest --watch",
    "test:changed-since": "jest --changedSince=origin/main --detectOpenHandles --forceExit --coverage --collectCoverageFrom=./src/**",
    "validate-semantic-release": "semantic-release --dry-run",
    "generate-semantic-release": "semantic-release",
    "test-coverage": "jest --coverage",
    "generate-dev-version": "node update-dev-version.mjs"
  },
  "dependencies": {
    "axios": "1.7.9",
    "clsx": "2.1.1",
    "funds-web": "^1.9.0",
    "next": "14.2.24",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.1",
    "sass": "1.85.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "resolutions": {
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "//NOTE-1": "fix for @isaacs/cliui v8.0.2 issue, see https://github.com/isaacs/jackspeak/issues/5",
    "jackspeak": "2.1.1"
  },
  "development": {
    "version": "1.4.0+dev.30"
  }
}
